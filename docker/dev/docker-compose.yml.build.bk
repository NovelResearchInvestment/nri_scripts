version: '3.9'

services:

  centos-dev:
    build:
      context: './centos-dev'
      dockerfile: Dockerfile
    container_name: centos-dev
    working_dir: /root
    tty: true
    extra_hosts:
      - "raw.githubusercontent.com:199.232.28.133"
    command: ["/bin/sh"]

